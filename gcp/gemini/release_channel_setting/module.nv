variables:
  location:
    type: string
    description: "Resource ID segment making up resource 'name'. It identifies the resource within its parent collection as described in https://google.aip.dev/122."
  release_channel_setting_id:
    type: string
    description: "Id of the Release Channel Setting."
  id:
    type: string
    description: "Optional property for ReleaseChannelSetting"
    default: null
  labels:
    type: object
    description: "Labels as key value pairs.  **Note**: This field is non-authoritative, and will only manage the labels present in your configuration. Please refer to the field 'effective_labels' for all of the labels present on the resource."
    default: null
  project:
    type: string
    description: "Optional property for ReleaseChannelSetting"
    default: null
  release_channel:
    type: string
    description: "Release channel to be used. Possible values: STABLE EXPERIMENTAL"
    default: null
  timeouts:
    type: list
    description: "Nested block: timeouts"
    default: []

resources:
  main:
    type: gcp:gemini:ReleaseChannelSetting
    properties:
      location: ${var.location}
      release_channel_setting_id: ${var.release_channel_setting_id}
      id: ${var.id}
      labels: ${var.labels}
      project: ${var.project}
      release_channel: ${var.release_channel}
      dynamic:
        timeouts:
          for_each: ${var.timeouts}
          content:
            create: ${each.value.create}
            delete: ${each.value.delete}
            update: ${each.value.update}
