variables:
  code_repository_index:
    type: string
    description: "Required property for RepositoryGroupIamMember"
  member:
    type: string
    description: "Required property for RepositoryGroupIamMember"
  repository_group_id:
    type: string
    description: "Required property for RepositoryGroupIamMember"
  role:
    type: string
    description: "Required property for RepositoryGroupIamMember"
  id:
    type: string
    description: "Optional property for RepositoryGroupIamMember"
    default: null
  location:
    type: string
    description: "Optional property for RepositoryGroupIamMember"
    default: null
  project:
    type: string
    description: "Optional property for RepositoryGroupIamMember"
    default: null
  condition:
    type: list
    description: "Nested block: condition"
    default: []

resources:
  main:
    type: gcp:gemini:RepositoryGroupIamMember
    properties:
      code_repository_index: ${var.code_repository_index}
      member: ${var.member}
      repository_group_id: ${var.repository_group_id}
      role: ${var.role}
      id: ${var.id}
      location: ${var.location}
      project: ${var.project}
      dynamic:
        condition:
          for_each: ${var.condition}
          content:
            description: ${each.value.description}
            expression: ${each.value.expression}
            title: ${each.value.title}

outputs:
  etag:
    description: "Output: etag"
    value: ${resources.main.etag}
  id:
    description: "Output: id"
    value: ${resources.main.id}
  location:
    description: "Output: location"
    value: ${resources.main.location}
  project:
    description: "Output: project"
    value: ${resources.main.project}
