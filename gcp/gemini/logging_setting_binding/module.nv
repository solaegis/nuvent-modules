variables:
  logging_setting_id:
    type: string
    description: "Resource ID segment making up resource 'name'. It identifies the resource within its parent collection as described in https://google.aip.dev/122."
  setting_binding_id:
    type: string
    description: "Id of the setting binding."
  target:
    type: string
    description: "Target of the binding."
  id:
    type: string
    description: "Optional property for LoggingSettingBinding"
    default: null
  labels:
    type: object
    description: "Labels as key value pairs.  **Note**: This field is non-authoritative, and will only manage the labels present in your configuration. Please refer to the field 'effective_labels' for all of the labels present on the resource."
    default: null
  location:
    type: string
    description: "Resource ID segment making up resource 'name'. It identifies the resource within its parent collection as described in https://google.aip.dev/122."
    default: null
  product:
    type: string
    description: "Product type of the setting binding. Possible values: [\"GEMINI_CODE_ASSIST\"]"
    default: null
  project:
    type: string
    description: "Optional property for LoggingSettingBinding"
    default: null
  timeouts:
    type: list
    description: "Nested block: timeouts"
    default: []

resources:
  main:
    type: gcp:gemini:LoggingSettingBinding
    properties:
      logging_setting_id: ${var.logging_setting_id}
      setting_binding_id: ${var.setting_binding_id}
      target: ${var.target}
      id: ${var.id}
      labels: ${var.labels}
      location: ${var.location}
      product: ${var.product}
      project: ${var.project}
      dynamic:
        timeouts:
          for_each: ${var.timeouts}
          content:
            create: ${each.value.create}
            delete: ${each.value.delete}
            update: ${each.value.update}
