#------------------------------------------------------------------------------
# gcp/iap/web_region_forwarding_rule_service_iam_policy/module.nv
# yaml-language-server: $schema=https://raw.githubusercontent.com/solaegis/nuvent/refs/heads/main/tools/vscode-extension/schemas/nuvent-manifest.schema.json
#------------------------------------------------------------------------------

variables:
  forwarding_rule_region_service_name:
    type: string
    description: "Required property for WebRegionForwardingRuleServiceIamPolicy"
  policy_data:
    type: string
    description: "Required property for WebRegionForwardingRuleServiceIamPolicy"
  id:
    type: string
    description: "Optional property for WebRegionForwardingRuleServiceIamPolicy"
    default: null
  project:
    type: string
    description: "Optional property for WebRegionForwardingRuleServiceIamPolicy"
    default: null
  region:
    type: string
    description: "Optional property for WebRegionForwardingRuleServiceIamPolicy"
    default: null

resources:
  main:
    type: gcp:iap:WebRegionForwardingRuleServiceIamPolicy
    properties:
      forwarding_rule_region_service_name: ${var.forwarding_rule_region_service_name}
      policy_data: ${var.policy_data}
      id: ${var.id}
      project: ${var.project}
      region: ${var.region}

outputs:
  etag:
    description: "Output: etag"
    value: ${resources.main.etag}
  id:
    description: "Output: id"
    value: ${resources.main.id}
  project:
    description: "Output: project"
    value: ${resources.main.project}
  region:
    description: "Output: region"
    value: ${resources.main.region}
