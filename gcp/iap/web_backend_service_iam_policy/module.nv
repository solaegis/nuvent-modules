#------------------------------------------------------------------------------
# gcp/iap/web_backend_service_iam_policy/module.nv
# yaml-language-server: $schema=https://raw.githubusercontent.com/solaegis/nuvent/refs/heads/main/tools/vscode-extension/schemas/nuvent-manifest.schema.json
#------------------------------------------------------------------------------

variables:
  policy_data:
    type: string
    description: "Required property for WebBackendServiceIamPolicy"
  web_backend_service:
    type: string
    description: "Required property for WebBackendServiceIamPolicy"
  id:
    type: string
    description: "Optional property for WebBackendServiceIamPolicy"
    default: null
  project:
    type: string
    description: "Optional property for WebBackendServiceIamPolicy"
    default: null

resources:
  main:
    type: gcp:iap:WebBackendServiceIamPolicy
    properties:
      policy_data: ${var.policy_data}
      web_backend_service: ${var.web_backend_service}
      id: ${var.id}
      project: ${var.project}

outputs:
  etag:
    description: "Output: etag"
    value: ${resources.main.etag}
  id:
    description: "Output: id"
    value: ${resources.main.id}
  project:
    description: "Output: project"
    value: ${resources.main.project}
