variables:
  policy_data:
    type: string
    description: "Required property for WebIamPolicy"
  id:
    type: string
    description: "Optional property for WebIamPolicy"
    default: null
  project:
    type: string
    description: "Optional property for WebIamPolicy"
    default: null

resources:
  main:
    type: gcp:iap:WebIamPolicy
    properties:
      policy_data: ${var.policy_data}
      id: ${var.id}
      project: ${var.project}

outputs:
  etag:
    description: "Output: etag"
    value: ${resources.main.etag}
  id:
    description: "Output: id"
    value: ${resources.main.id}
  project:
    description: "Output: project"
    value: ${resources.main.project}
