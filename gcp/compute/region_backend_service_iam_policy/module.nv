variables:
  name:
    type: string
    description: "Required property for RegionBackendServiceIamPolicy"
  policy_data:
    type: string
    description: "Required property for RegionBackendServiceIamPolicy"
  id:
    type: string
    description: "Optional property for RegionBackendServiceIamPolicy"
    default: null
  project:
    type: string
    description: "Optional property for RegionBackendServiceIamPolicy"
    default: null
  region:
    type: string
    description: "Optional property for RegionBackendServiceIamPolicy"
    default: null

resources:
  main:
    type: gcp:compute:RegionBackendServiceIamPolicy
    properties:
      name: ${var.name}
      policy_data: ${var.policy_data}
      id: ${var.id}
      project: ${var.project}
      region: ${var.region}

outputs:
  etag:
    description: "Output: etag"
    value: ${resources.main.etag}
  id:
    description: "Output: id"
    value: ${resources.main.id}
  project:
    description: "Output: project"
    value: ${resources.main.project}
  region:
    description: "Output: region"
    value: ${resources.main.region}
