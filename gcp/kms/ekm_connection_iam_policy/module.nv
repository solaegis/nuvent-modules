variables:
  name:
    type: string
    description: "Required property for EkmConnectionIamPolicy"
  policy_data:
    type: string
    description: "Required property for EkmConnectionIamPolicy"
  id:
    type: string
    description: "Optional property for EkmConnectionIamPolicy"
    default: null
  location:
    type: string
    description: "Optional property for EkmConnectionIamPolicy"
    default: null
  project:
    type: string
    description: "Optional property for EkmConnectionIamPolicy"
    default: null

resources:
  main:
    type: gcp:kms:EkmConnectionIamPolicy
    properties:
      name: ${var.name}
      policy_data: ${var.policy_data}
      id: ${var.id}
      location: ${var.location}
      project: ${var.project}
