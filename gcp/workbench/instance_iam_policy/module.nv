variables:
  name:
    type: string
    description: "Required property for InstanceIamPolicy"
  policy_data:
    type: string
    description: "Required property for InstanceIamPolicy"
  id:
    type: string
    description: "Optional property for InstanceIamPolicy"
    default: null
  location:
    type: string
    description: "Optional property for InstanceIamPolicy"
    default: null
  project:
    type: string
    description: "Optional property for InstanceIamPolicy"
    default: null

resources:
  main:
    type: gcp:workbench:InstanceIamPolicy
    properties:
      name: ${var.name}
      policy_data: ${var.policy_data}
      id: ${var.id}
      location: ${var.location}
      project: ${var.project}

outputs:
  etag:
    description: "Output: etag"
    value: ${resources.main.etag}
  id:
    description: "Output: id"
    value: ${resources.main.id}
  location:
    description: "Output: location"
    value: ${resources.main.location}
  project:
    description: "Output: project"
    value: ${resources.main.project}
